    "@types/react-dom": "^16.8.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.1",
    "jquery": "^3.2.1",
    "jquery-ujs": "^1.2.2",
    "popper.js": "^1.12.9",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.8.4",
    "typescript": "^3.4.5"
