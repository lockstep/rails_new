{
  "name": "rails_new",
  "private": true,
  "engines": {
    "node": "^16.13.0",
    "yarn": ">=1.9.0"
  },
  "config": {
    "files": "app/javascript/**/*.{js,jsx,ts,tsx} app/assets/javascripts/**/*.{js,jsx,ts,tsx}"
  },
  "scripts": {
    "lint-check": "eslint --format codeframe $npm_package_config_files && prettier --list-different $npm_package_config_files",
    "lint-write": "eslint --fix $npm_package_config_files && prettier --write $npm_package_config_files",
    "types-watch": "tsc --watch",
    "test": "jest --passWithNoTests",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@sentry/browser": "^5.11.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.1",
    "esbuild": "^0.14.11",
    "jquery": "^3.2.1",
    "jquery-ujs": "^1.2.2",
    "popper.js": "^1.12.9",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.8.4",
    "react_ujs": "^2.5.0",
    "typescript": "^3.7.0"
  },
  "devDependencies": {
    "@prettier/plugin-ruby": "^1.0.1",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.0",
    "react-test-renderer": "^16.13.1"
  },
  "resolutions": {
    "node-sass": "^6.0.1"
  }
}
